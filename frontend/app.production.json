{
  "expo": {
    "name": "SlimIQ",
    "slug": "slimiq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "slimiq",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#14B8A6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.slimiq.app",
      "icon": "./assets/images/icon.png",
      "infoPlist": {
        "CFBundleIconName": "AppIcon"
      },
      "buildNumber": "1"
    },
    "android": {
      "package": "com.slimiq.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#14B8A6"
      },
      "icon": "./assets/images/icon.png",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "ENV": "production"
    }
  }
}